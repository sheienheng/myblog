
<% include header %>
<% docs.forEach(function (doc) { %>
<p><h2><a href="/u/<%= doc.name %>/<%= doc.title %>/<%= doc.time %>"> <%= doc.title %></a></h2></p>
<p class="info">
    作者：<a href="/u/<%= doc.name %>"><%= doc.name %></a> |
    日期：<%= doc.time %> |
    标签：
    <% doc.tags.forEach(function (tag) { %>
        <% if(tag){ %>
            <a class="tag" href="/tags/<%= tag %>"><%= tag %></a>
        <% }%>
    <% }) %>
</p>
<p><%- doc.content %></p>
<% }) %>
<% include paging %>
<% include footer %>


